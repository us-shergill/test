var mailText = worksheet.getCell(worksheet.getRow(rowNum).getCell(colNum)._address).value;
if (mailText != null && typeof mailText === 'string') {
    mailText = mailText.substring(0, 100); // Only substring if it's a string
    var mailValue: number = +mailText; // Convert to number
} else {
    console.warn(`Cell at (${rowNum}, ${colNum}) has an invalid or empty value.`);
    var mailValue: number = 0; // Handle the case where value is null or invalid
}
